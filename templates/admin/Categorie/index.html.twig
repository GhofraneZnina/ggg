{% extends "base.html.twig" %}
{% block title %}Liste des Categories
{% endblock %}
{% block pageTitle %}Liste des Categories
{% endblock %}
{% block levelOne %}
	<li class="breadcrumb-item">
		<a href="{{ path('app_admin_cotisation_list') }}">cotisations</a>
	</li>
{% endblock %}
{% block levelTwo %}{% endblock %}
{% block levelThree %}{% endblock %}
{% block btnLink %}



{% endblock %}


{% block body %}
	{#  Show alert : START #}
	{% for message in app.flashes('success') %}
		<div class="card-alert card green">
			<div class="card-content white-text">
				<p>
					{{ message }}</p>
			</div>
			<button type="button" class="close white-text" data-dismiss="alert" aria-label="Close">
				<span aria-hidden="true">×</span>
			</button>
		</div>
	{% endfor %}
	{% for message in app.flashes('error') %}
		<div class="card-alert card red">
			<div class="card-content white-text">
				<p>
					{{ message }}</p>
			</div>
			<button type="button" class="close white-text" data-dismiss="alert" aria-label="Close">
				<span aria-hidden="true">×</span>
			</button>
		</div>
	{% endfor %}
	{#  Show alert : END  #}
	<section class="users-list-wrapper section">
		<div class="users-list-table">
			<div class="card">
				<div
					class="card-content">
					<!-- datatable start -->
 <div class="row">
            <div class="col s12">
              <table id="page-length-option" class="display">
							<thead>
								<tr>
									<th></th>
									<th>id</th>
									<th>intitule</th>
									
									<th>La categorie d'age</th>
									

								</tr>
							</thead>
							<tbody>
								{% for categorie in categorie %}
									<tr>
										<td></td>
										<td>{{ categorie.id }}</td>
										<td>{{ categorie.intitule}}</td>
										
										<td>{{ categorie.categorieAge }}</td>
										

									</tr>
								{% endfor %}
							</tbody>
						</table>
					</div>	</div>
					<!-- datatable ends -->
				</div>
			</div>
		</div>
	</section>
	
{% endblock %}
